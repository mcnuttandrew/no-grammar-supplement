  "gemSpecs": [
    {
      "meta": {"name": "Change all at once"},
      "timeline": {
        "sync": [
          {"component": {"axis": "x"}, "timing": {"duration": {"ratio": 1}}},
          {"component": {"axis": "y"}, "timing": {"duration": {"ratio": 1}}},
          {"component": {"legend": "legend0"}, "timing": {"duration": {"ratio": 1}}},
          {"component": "view", "timing": {"duration": {"ratio": 1}}},
          {
            "component": {"mark": "marks"},
            "change": {"data": ["Player"]},
            "timing": {"duration": {"ratio": 1}}
          }
        ]
      },
      "totalDuration": 2000
    },
    {
      "meta": {"name": "Exit -> Update -> Enter"},
      "timeline": {
        "concat": [
          {
            "component": {"mark": "marks"},
            "timing": {"duration": {"ratio": 0.3}},
            "change": {
              "data": { "keys": ["Player"], "enter": false, "update": false},
              "encode": { "enter": false, "update": false},
              "scale": false
            }
          },
          {
            "sync": [
              {"component": {"axis": "x"}, "timing": {"duration": {"ratio": 0.4}}},
              {"component": {"axis": "y"}, "timing": {"duration": {"ratio": 0.4}}},
              {"component": {"legend": "legend0"}, "timing": {"duration": {"ratio": 0.4}}},
              {"component": "view", "timing": {"duration": {"ratio": 0.4}}},
              {
                "component": {"mark": "marks"},
                "timing": {"duration": {"ratio": 0.4}},
                "change": {
                  "data": { "keys": ["Player"], "enter": false },
                    "encode": {"enter": false}
                }
              }
            ]
          },
          {
            "component": {"mark": "marks"},
            "timing": {"duration": {"ratio": 0.3}},
            "change": {
              "data": { "keys": ["Player"] }
            }
          }
        ]
      },
      "totalDuration": 2000
    }

